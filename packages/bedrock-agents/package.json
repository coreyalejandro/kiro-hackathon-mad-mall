{
  "name": "@madmall/bedrock-agents",
  "version": "0.0.0",
  "description": "AWS Bedrock AI agents for MADMall platform",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./agents": {
      "types": "./lib/agents/index.d.ts",
      "import": "./lib/agents/index.js",
      "require": "./lib/agents/index.js"
    },
    "./workflows": {
      "types": "./lib/workflows/index.d.ts",
      "import": "./lib/workflows/index.js",
      "require": "./lib/workflows/index.js"
    },
    "./types": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/types/index.js",
      "require": "./lib/types/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf lib",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.658.1",
    "@aws-sdk/client-bedrock-agent": "^3.658.1",
    "@aws-sdk/client-bedrock-runtime": "^3.658.1",
    "@madmall/shared-types": "workspace:*",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^18",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "keywords": [
    "aws",
    "bedrock",
    "ai",
    "agents",
    "madmall",
    "wellness"
  ]
}